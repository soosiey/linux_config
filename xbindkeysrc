# Increase volume
sh -c "pactl set-sink-mute 0 false; pactl set-sink-volume 0 +5%"
    XF86AudioRaiseVolume
#Decrease volume
sh -c "pactl set-sink-mute 0 false; pactl set-sink-volume 0 -5%"
    XF86AudioLowerVolume
sh -c "light -U 10"
    XF86MonBrightnessDown
sh -c "light -A 10"
    XF86MonBrightnessUp
